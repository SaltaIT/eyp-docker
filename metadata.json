{
  "name": "eyp-docker",
  "version": "0.2.6",
  "author": "Jordi Prats",
  "summary": "docker service and container management",
  "license": "Apache-2.0",
  "source": "https://github.com/SaltaIT/eyp-docker",
  "project_page": "https://github.com/SaltaIT/eyp-docker",
  "issues_url": "https://github.com/SaltaIT/eyp-docker/issues",
  "dependencies": [
